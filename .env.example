# ====================================
# CADLift Frontend Configuration
# ====================================
# Copy this file to .env.local and update values for your environment

# ============ API CONFIGURATION ============
# Backend API URL (leave empty to use local mock mode)
VITE_API_BASE_URL=http://localhost:8000

# ============ FEATURES ============
# Enable console logging for job operations
VITE_ENABLE_JOB_LOGS=true

# Enable local job simulation when API is unavailable
VITE_ENABLE_LOCAL_JOBS=true

# Demo mode - locks GPU features and shows GitHub download prompt
VITE_DEMO_MODE=false
